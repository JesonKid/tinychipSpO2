setup()
{
  __message "----- setup hardware for Flashloader -----\n";
  /*Wait*/ 
  __delay(5);
  __emulatorSpeed(0);
  
}

execUserPreload()
{
  __message "----- execUserPreload  -----\n";
  setup();
}

execUserFlashInit()  // Called by debugger before loading flash loader in RAM.
{
  __message "----- execUserFlashInit -----\n";
  setup();
}

